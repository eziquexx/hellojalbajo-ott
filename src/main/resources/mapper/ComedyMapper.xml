<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hellojalbajo.ott.comedy.ComedyMapper">
	<select id="getComedyById" resultType="com.hellojalbajo.ott.Genre">
		SELECT * FROM ottdb_movie WHERE genre = 'comedy' AND id = #{id}
	</select>
	<select id="getComedyList" resultType="com.hellojalbajo.ott.Genre">
		SELECT * FROM ottdb_movie WHERE genre = 'comedy'
	</select>
</mapper>